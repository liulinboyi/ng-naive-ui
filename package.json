    "build:docs-gh-pages": "docgeni build --configuration production --base-href=/ng-naive-ui/ --deploy-url=/ng-naive-ui/",
    "test": "ng test",
    "lint": "ng lint",
    "prepare": "husky install",
    "pretty": "pretty-quick",
    "deploy": "node script/deploy.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.1",
    "@angular/common": "~13.0.1",
